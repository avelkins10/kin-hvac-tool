  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && (timeout 120 npx prisma migrate deploy || echo 'Migrations skipped - timeout or already applied') && next build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 prisma generate && next dev --webpack -p 3000",
    "dev:webpack": "NODE_TLS_REJECT_UNAUTHORIZED=0 prisma generate && next dev --webpack -p 3000",
    "dev:clean": "(lsof -ti:3000; lsof -ti:3002) 2>/dev/null | xargs kill -9 2>/dev/null; rm -rf .next && pnpm run dev",
    "dev:serve": "pnpm run build && next start -p 3000",
    "lint": "eslint .",
    "start": "next start",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:supabase": "bash scripts/run-supabase-migration.sh",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "create-admin": "tsx scripts/create-admin-user.ts",
    "migrate-db": "tsx scripts/migrate-to-supabase.ts",
    "verify-migration-env": "npx tsx scripts/verify-migration-env.ts",
    "test-supabase-connection": "tsx scripts/test-supabase-connection.ts",
    "supabase:setup": "tsx scripts/setup-supabase-complete.ts",
    "seed": "tsx scripts/seed-admin-defaults.ts"
